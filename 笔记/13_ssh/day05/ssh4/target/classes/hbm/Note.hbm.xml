<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.tedu.note.entity.Note"
			table="cn_note">
		<id name="id" column="cn_note_id"></id>
		<property name="notebookId" 	
			column="cn_notebook_id"></property>
		<property name="userId" 
			column="cn_user_id"></property>
		<property name="typeId"
			column="cn_note_type_id"></property>
		<property name="statusId"
			column="cn_note_status_id"></property>
		<property name="title"
			column="cn_note_title"></property>
		<property name="body"
			column="cn_note_body"></property>
		<property name="createTime" 
			column="cn_note_create_time"></property>
		<property name="lastModifyTime"
			column="cn_note_last_modify_time"></property>
	</class> 
</hibernate-mapping>






